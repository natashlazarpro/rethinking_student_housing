@inbook{gwoscStudentsHousingSituation2024,
  title = {Students' Housing Situation},
  booktitle = {Social and {{Economic Conditions}} of {{Student Life}} in {{Europe}}: {{Eurostudent}} 8 {{Synopsis}} of {{Indicators}} 2021-2024},
  author = {Gwosc, Christoph},
  year = 2024,
  publisher = {wbv Media GmbH \& Co. KG},
  urldate = {2025-06-04},
  abstract = {Housing is a key element for living and studying, which can help fulfil a plethora of needs, such as for physical and mental shelter, social needs for communication and integration, or for socio-academic integration. This chapter analyses the housing situation of students in the EUROSTUDENT countries. The analysis provides an overview of how students allocate themselves into five different types of housing, which are characterised by different up- and downsides. Some forms of housing, such as living with parents and student accommodation, that are of particular importance in different countries are analysed in greater depth. As the home is also a place of study, the question of the extent to which different forms of accommodation offer students access to sufficient internet connection and a quiet place to study is investigated. The chapter also analyses how different forms of housing affect the time students spend commuting between their home and their higher education institution.},
  collaborator = {Hauschildt, Kristina},
